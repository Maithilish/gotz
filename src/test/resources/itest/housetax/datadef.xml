<?xml version="1.0" encoding="UTF-8"?>

<dataDefs xmlns="http://codetab.org/gotz" xmlns:xf="http://codetab.org/xfields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://codetab.org/gotz gotz.xsd">

    <dataDef name="taxtable">
        <axis name="col">
            <xf:fields>
                <xf:query region="table:contains(Taxes)"
                    field="tbody > tr:nth-child(1) > th:nth-child(%{col.index})" />
            </xf:fields>
            <member name="year" index="2">
                <xf:fields>
                    <xf:breakAfter value="2017" />
                </xf:fields>
            </member>
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query region="table:contains(Taxes)"
                    field="tbody > tr:nth-child(%{row.index}) > td:nth-child(1)" />
            </xf:fields>
            <member name="item" index="2">
                <xf:fields>
                    <xf:breakAfter value="GF" />
                </xf:fields>
            </member>
            <filter axis="row">
                <xf:fields>
                    <xf:filters type="value">
                        <xf:filter name="blank" pattern="" />
                    </xf:filters>
                </xf:fields>
            </filter>
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query region="table:contains(Taxes)"
                    field="tbody > tr:nth-child(%{row.index}) > td:nth-child(%{col.index})" />
            </xf:fields>
        </axis>
    </dataDef>

</dataDefs>
