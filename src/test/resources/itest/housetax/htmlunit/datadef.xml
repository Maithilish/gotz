<?xml version="1.0" encoding="UTF-8"?>

<datadefs xmlns="http://codetab.org/gotz" xmlns:xf="http://codetab.org/xfields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://codetab.org/gotz gotz.xsd">

    <datadef name="taxtable">
        <axis name="col">
            <xf:fields>
                <xf:query region="//table" field="*//tr[1]/th[%{col.index}+1]" />
            </xf:fields>
            <member name="year" >
                <xf:fields>
                    <xf:breakAfter value="2017" />
                </xf:fields>
            </member>
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query region="//table" field="*//tr[%{row.index}+1]/td[1]" />
            </xf:fields>
            <member name="item" >
                <xf:fields>
                    <xf:breakAfter value="GF" />
                </xf:fields>
            </member>
            <filter axis="row">
                <xf:fields>
                    <xf:filters type="value">
                        <xf:filter name="blank" pattern="" />
                    </xf:filters>
                </xf:fields>
            </filter>
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query region="//table"
                    field="*//*[.='%{row.value}']/following-sibling::td[%{col.index}]" />
            </xf:fields>
        </axis>
    </datadef>

</datadefs>
