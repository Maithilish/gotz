<?xml version="1.0" encoding="UTF-8"?>

<gotz xmlns="http://codetab.org/gotz" xmlns:xf="http://codetab.org/xfields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://codetab.org/gotz gotz.xsd">

    <dataDef name="finlocators">
        <xf:fields>
            <xf:member name="bs">
                <xf:group>bs</xf:group>
            </xf:member>
            <xf:member name="pl">
                <xf:group>pl</xf:group>
            </xf:member>
        </xf:fields>

        <axis name="col">
            <xf:fields>
                <xf:script script="configs.getRunDateTime()" />
            </xf:fields>
            <member name="date" />
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query
                    region="#page_links > table > tbody > tr > td:nth-child(4) > ul"
                    field="li:nth-child(%{row.index}) > a[href]" />
            </xf:fields>
            <member name="bs" index="2" />
            <member name="pl" index="3" />
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query
                    region="#page_links > table > tbody > tr > td:nth-child(4) > ul"
                    field="li:nth-child(%{row.index}) > a[href]"
                    attribute="href">
                </xf:query>
                <xf:prefix>src/test/resources/itest/itc/page/</xf:prefix>
            </xf:fields>
        </axis>
    </dataDef>
</gotz>


