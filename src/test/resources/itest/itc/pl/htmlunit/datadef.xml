<?xml version="1.0" encoding="UTF-8"?>

<gotz xmlns="http://codetab.org/gotz" xmlns:xf="http://codetab.org/xfields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://codetab.org/gotz gotz.xsd">

    <dataDef name="pl">
        <axis name="col">
            <xf:fields>
                <xf:query
                    region="//table//*[count(*)=0 and contains(.,'Employee Cost')]/ancestor::table[1]"
                    field="*//tr[1]/td[%{col.index}+1]" />
            </xf:fields>
            <member name="year">
                <xf:fields>
                    <xf:indexRange value="1-5" />
                </xf:fields>
            </member>
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query
                    region="//table//*[count(*)=0 and contains(.,'Employee Cost')]/ancestor::table[1]"
                    field="*//tr[%{row.index}+0]/td[1]" />
            </xf:fields>
            <member name="item" index="6">
                <xf:fields>
                    <xf:breakAfter value="Book Value (Rs)" />
                </xf:fields>
            </member>
            <filter axis="row">
                <xf:fields>
                    <xf:filters type="value">
                        <xf:filter name="blank" pattern="" />
                        <xf:filter name="f1" pattern="Expenditure" />
                        <xf:filter name="f2"
                            pattern="Per share data (annualised)" />
                    </xf:filters>
                </xf:fields>
            </filter>
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query
                    region="//table//*[count(*)=0 and contains(.,'Employee Cost')]/ancestor::table[1]"
                    field="*//*[.='%{row.value}']/following-sibling::td[%{col.index}]" />
            </xf:fields>
        </axis>
    </dataDef>

</gotz>
