<?xml version="1.0" encoding="UTF-8"?>

<picks>
	<properties>
		<property name="mc" value="tprefix/" />
	</properties>
	<datadefs>
		<datadef name="tdatadef1" parser="in.m.picks.ext.HtmlDataParser">
			<axes>
				<axis name="col">
					<afield name="region" group="query" value="col-query-1" />
					<afield name="field" group="query" value="col-query-2" />
					<member name="tname1">
						<afield name="startIndex" value="1" />
						<afield name="endIndex" value="2" />
					</member>
				</axis>
				<axis name="row">
					<afield name="region" group="query" value="row-query-1" />
					<afield name="field" group="query" value="row-query-2" />
					<member name="tname2">
						<afield name="startIndex" value="8" />
						<afield name="endIndex" value="9" />
					</member>
				</axis>
				<axis name="fact">
					<afield name="region" group="query" value="fact-query-1" />
					<afield name="field" group="query" value="fact-query-2" />
				</axis>
			</axes>
		</datadef>
		<datadef name="tdatadef2" parser="in.m.picks.ext.HtmlDataParser">
			<axes>
				<axis name="col">
					<afield name="region" group="query" value="col-query-1" />
					<afield name="field" group="query" value="col-query-2" />
					<member name="tname1">
						<afield name="startIndex" value="1" />
						<afield name="endIndex" value="3" />
					</member>
				</axis>
				<axis name="row">
					<afield name="region" group="query" value="row-query-1" />
					<afield name="field" group="query" value="row-query-2" />
					<member name="tname2" />
				</axis>
				<axis name="fact">
					<afield name="region" group="query" value="fact-query-1" />
					<afield name="field" group="query" value="fact-query-2" />
				</axis>
			</axes>
		</datadef>
		<datadef name="tdatadef3" parser="in.m.picks.ext.HtmlDataParser">
			<axes>
				<axis name="col">
					<afield name="region" group="query" value="html/body/table" />
					<afield name="field" group="query" value="tbody/tr[1]/td[col.index]" />
					<member name="tname1">
						<afield name="startIndex" value="2" />
						<afield name="endIndex" value="3" />
					</member>
				</axis>
				<axis name="row">
					<afield name="region" group="query" value="html/body/table" />
					<afield name="field" group="query" value="tbody/tr[row.index]/td[1]" />
					<member name="tname2">
						<afield name="startIndex" value="4" />
						<afield name="endIndex" value="5" />
					</member>
				</axis>
				<axis name="fact">
					<afield name="region" group="query" value="html/body/table" />
					<afield name="field" group="query"
						value="tbody/tr[row.index]/td[col.index]" />
				</axis>
			</axes>
		</datadef>
		<datadef name="tdatadef4" parser="in.m.picks.ext.HtmlLocatorParser"
			type="locator">
			<axes>
				<axis name="col">
					<afield name="script" group="script" value="configs.getRunDate()" />
					<member name="date" />
				</axis>
				<axis name="row">
					<afield name="region" group="query" value="html/body" />
					<afield name="field" group="query" value="a[contains(.,'%{row.match}')]" />
					<member name="bs" match="Balance Sheet" group="bs" />
				</axis>
				<axis name="fact">
					<afield name="region" group="query" value="html/body" />
					<afield name="field" group="query"
						value="a[contains(.,'%{row.match}')]/@href" />
					<afield name="prefix" value="${mc}" group="prefix" />
				</axis>
			</axes>
		</datadef>
	</datadefs>
</picks>

