<?xml version="1.0" encoding="UTF-8"?>
<datadefs xmlns="http://codetab.org/picks" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://codetab.org/picks picks.xsd">
	<datadef name="taxestable" parser="in.m.picks.ext.HtmlDataParser">
		<afield name="datadef" group="debugstate" value="false" />
		<afield name="dataprototype" group="debugstate" value="false" />
		<axis name="col">
			<afield name="region" group="query" value="//table" />
			<afield name="field" group="query" value="*//tr[1]/th[%{col.index}+1]" />
			<member name="year">
				<afield name="indexRange" value="1-3" />
			</member>
		</axis>
		<axis name="row">
			<afield name="region" group="query" value="//table" />
			<afield name="field" group="query" value="*//tr[%{row.index}+1]/td[1]" />
			<member name="item">
				<afield name="indexRange" value="1-2" />
			</member>
			<filter axis="row">
				<afield name="blank" value="" group="value" />
			</filter>
		</axis>
		<axis name="fact">
			<afield name="region" group="query" value="//table" />
			<afield name="field" group="query"
				value="*//*[.='%{row.value}']/following-sibling::td[%{col.index}]" />
		</axis>
	</datadef>
</datadefs>
