<?xml version='1.0'?>

<afieldslist xmlns="http://codetab.org/picks"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://codetab.org/picks picks.xsd">
	<afields className="in.m.picks.model.Locator">
		<afield name="loader" value="in.m.picks.ext.HtmlLoader" group="loader" />
		<afield name="parser" value="in.m.picks.ext.HtmlDataParser"
			group="parser" />
		<afield name="transformer" value="in.m.picks.ext.CsvTransformer"
			group="transformer" />
		<afield name="fileappender" value="in.m.picks.appender.FileAppender"
			group="appender" />
		<afield name="taxes" value="taxestable" group="datadef" />
		<afield name="persist" value="true" group="html" />
		<afield name="persistdata" value="true" group="html" />
	</afields>
</afieldslist>

