<?xml version='1.0'?>

<fields xmlns="http://codetab.org/gotz">
    <fields name="class" value="org.codetab.gotz.model.Locator">
        <fields name="group" value="html">
            <field name="loader" value="org.codetab.gotz.ext.HtmlLoader" />
            <fields name="group" value="datadef">
                <field name="datadef" value="taxestable" />
                <field name="parser" value="org.codetab.gotz.ext.HtmlDataParser" />
                <field name="transformer" value="org.codetab.gotz.ext.CsvTransformer" />
                <field name="appender"
                    value="org.codetab.gotz.appender.FileAppender" />
                <field name="logstate" value="true" />
            </fields>
            <field name="persist" value="true" />
            <field name="persistdata" value="true" />
        </fields>
    </fields>
</fields>
