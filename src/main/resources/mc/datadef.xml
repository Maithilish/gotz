<?xml version="1.0" encoding="UTF-8"?>
<datadefs>
	<datadef name="finlocators" parser="in.m.picks.ext.HtmlLocatorParser"
		type="locator">

		<axis name="col">
			<afield name="script" group="script" value="configs.getRunDate()" />
			<member name="date" />
		</axis>
		<axis name="row">
			<afield name="region" group="query"
				value="//ul//*[count(*)=0 and contains(.,'Balance Sheet')]/ancestor::ul[1]" />
			<afield name="field" group="query" value="li/a[contains(.,'%{row.match}')]" />
			<member name="bs" match="Balance Sheet" group="bs" />
			<!-- <member name="pl" match="Profit &amp; Loss" group="pl" /> -->
		</axis>
		<axis name="fact">
			<afield name="region" group="query"
				value="//ul//*[count(*)=0 and contains(.,'Balance Sheet')]/ancestor::ul[1]" />
			<afield name="field" group="query"
				value="*//a[contains(.,'%{row.match}')]/@href" />
			<afield name="prefix" value="${mc}" group="prefix" />
		</axis>

	</datadef>
	<datadef name="bs" parser="in.m.picks.ext.HtmlDataParser">

		<axis name="col">
			<afield name="region" group="query"
				value="//table//*[count(*)=0 and contains(.,'Total Share Capital')]/ancestor::table[1]" />
			<afield name="field" group="query" value="*//tr[1]/td[%{col.index}+1]" />
			<member name="year">
				<afield name="startIndex" value="1" />
				<afield name="endIndex" value="2" />
			</member>
		</axis>
		<axis name="row">
			<afield name="region" group="query"
				value="//table//*[count(*)=0 and contains(.,'Total Share Capital')]/ancestor::table[1]" />
			<afield name="field" group="query" value="*//tr[%{row.index}+4]/td[1]" />
			<member name="item">
				<afield name="startIndex" value="1" />
				<afield name="endIndex" value="5" />
			</member>
			<filter axis="row">
				<afield name="blank" value="" group="value" />
				<afield name="f1" value="Sources Of Funds" group="value" />
				<afield name="f2" value="Application Of Funds" group="value" />
			</filter>
		</axis>
		<axis name="fact">
			<afield name="region" group="query"
				value="//table//*[count(*)=0 and contains(.,'Total Share Capital')]/ancestor::table[1]" />
			<afield name="field" group="query"
				value="*//*[.='%{row.value}']/following-sibling::td[%{col.index}]" />
		</axis>

	</datadef>
</datadefs>


