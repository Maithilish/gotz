<?xml version="1.0" encoding="UTF-8"?>

<datadefs xmlns="http://codetab.org/gotz" xmlns:xf="http://codetab.org/xfield"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://codetab.org/gotz gotz.xsd">

    <datadef name="finlocators">
        <xf:xfield>
            <xf:member name="bs">
                <xf:group>bs</xf:group>
            </xf:member>
            <xf:member name="pl">
                <xf:group>pl</xf:group>
            </xf:member>
        </xf:xfield>

        <axis name="col">
            <xf:xfield>
                <xf:script script="configs.getRunDateTime()" />
            </xf:xfield>
            <member name="date" />
        </axis>
        <axis name="row">
            <xf:xfield>
                <xf:query
                    region="#quickLinkPrc > table > tbody > tr > td:nth-child(4) > ul"
                    field="li:nth-child(%{row.index}) > a[href]" />
            </xf:xfield>
            <member name="bs" index="2" />
            <member name="pl" index="3" />
        </axis>
        <axis name="fact">
            <xf:xfield>
                <xf:query
                    region="#quickLinkPrc > table > tbody > tr > td:nth-child(4) > ul"
                    field="li:nth-child(%{row.index}) > a[href]"
                    attribute="href">
                </xf:query>
                <!-- uses local file (quote page) so no suffix  -->
                <xf:suffix></xf:suffix>
            </xf:xfield>
        </axis>
    </datadef>

