<?xml version="1.0" encoding="UTF-8"?>

<datadefs xmlns="http://codetab.org/gotz" xmlns:xf="http://codetab.org/xfields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://codetab.org/gotz gotz.xsd">

    <datadef name="finlocators">
        <xf:fields>
            <xf:member name="bs">
                <xf:group>bs</xf:group>
            </xf:member>
            <xf:member name="pl">
                <xf:group>pl</xf:group>
            </xf:member>
        </xf:fields>

        <axis name="col">
            <xf:fields>
                <xf:script script="configs.getRunDateTime()" />
            </xf:fields>
            <member name="date" />
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query
                    region="#quickLinkPrc > table > tbody > tr > td:nth-child(4) > ul"
                    field="li:nth-child(%{row.index}) > a[href]" />
            </xf:fields>
            <member name="bs" index="2" />
            <member name="pl" index="3" />
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query
                    region="#quickLinkPrc > table > tbody > tr > td:nth-child(4) > ul"
                    field="li:nth-child(%{row.index}) > a[href]"
                    attribute="href">
                </xf:query>
                <!-- uses local file (quote page) so no prefix  -->
                <xf:prefix></xf:prefix>
            </xf:fields>
        </axis>
    </datadef>

    <datadef name="quotePage">
        <axis name="col">
            <xf:fields>
                <xf:script script="document.getFromDate()" />
            </xf:fields>
            <member name="date" />
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query region="div#mktdet_2"
                    field="div:containsOwn(%{row.match})" />
            </xf:fields>
            <member name="MC" match="MARKET CAP" />
            <member name="EPS" match="EPS (TTM)" />
            <member name="PE" match="P/E" />
            <member name="PC" match="P/C" />
            <member name="BV" match="BOOK VALUE" />
            <member name="PB" match="PRICE/BOOK" />
            <member name="DIV" match="DIV (%)" />
            <member name="DY" match="DIV YIELD" />
            <member name="FV" match="FACE VALUE" />
            <member name="IND PE" match="INDUSTRY P/E" />
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query region="div#mktdet_2"
                    field="div:containsOwn(%{row.match}) + div" />
            </xf:fields>
        </axis>
    </datadef>
    <datadef name="price">
        <axis name="col">
            <xf:fields>
                <xf:script script="document.getFromDate()" />
            </xf:fields>
            <member name="date" />
        </axis>
        <axis name="row">
            <member name="Price" value="Price" />
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query region="div#Nse_Prc_tick_div" field="*" />
            </xf:fields>
        </axis>
    </datadef>

    <datadef name="bs">
        <axis name="col">
            <xf:fields>
                <xf:query region="table:contains(Sources Of Funds)"
                    field="tr:nth-child(1) > td:nth-child(%{col.index})" />
            </xf:fields>
            <member name="year">
                <xf:fields>
                    <xf:indexRange value="2-6" />
                </xf:fields>
            </member>
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query region="table:contains(Sources Of Funds)"
                    field="tr:nth-child(%{row.index}) > td:nth-child(1)" />
            </xf:fields>
            <member name="item" index="5">
                <xf:fields>
                    <xf:breakAfter value="Book Value (Rs)" />
                </xf:fields>
            </member>
            <filter axis="row">
                <xf:fields>
                    <xf:filters type="value">
                        <xf:filter name="blank" pattern="" />
                        <xf:filter name="f1" pattern="Sources Of Funds" />
                        <xf:filter name="f2" pattern="Application Of Funds" />
                    </xf:filters>
                </xf:fields>
            </filter>
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query region="table:contains(Sources Of Funds)"
                    field="tr:nth-child(%{row.index}) > td:nth-child(%{col.index})" />
            </xf:fields>
        </axis>
    </datadef>

    <datadef name="pl">
        <axis name="col">
            <xf:fields>
                <xf:query region="table:contains(Employee Cost)"
                    field="tr:nth-child(1) > td:nth-child(%{col.index})">
                </xf:query>
            </xf:fields>
            <member name="year">
                <xf:fields>
                    <xf:indexRange value="2-6" />
                </xf:fields>
            </member>
        </axis>
        <axis name="row">
            <xf:fields>
                <xf:query region="table:contains(Employee Cost)"
                    field="tr:nth-child(%{row.index}) > td:nth-child(1)">
                </xf:query>
            </xf:fields>
            <member name="item" index="6">
                <xf:fields>
                    <xf:breakAfter value="Book Value (Rs)" />
                </xf:fields>
            </member>
            <filter axis="row">
                <xf:fields>
                    <xf:filters type="value">
                        <xf:filter name="blank" pattern="" />
                        <xf:filter name="f1" pattern="Expenditure" />
                        <xf:filter name="f2"
                            pattern="Per share data (annualised)" />
                    </xf:filters>
                </xf:fields>
            </filter>
        </axis>
        <axis name="fact">
            <xf:fields>
                <xf:query region="table:contains(Employee Cost)"
                    field="tr:nth-child(%{row.index}) > td:nth-child(%{col.index})">
                </xf:query>
            </xf:fields>
        </axis>
    </datadef>

</datadefs>


